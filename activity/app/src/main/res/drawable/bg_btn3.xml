<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#16463d">
            </solid>
            <corners
                android:radius="8dp">
            </corners>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid
                android:color="#00ffaa">
            </solid>
            <corners
                android:radius="8dp">
            </corners>
        </shape>
    </item>

</selector>